{
  "name": "nitbit",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/((?!.*\\.\\w+$).*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.+\\.[a-zA-Z\\d]+)$",
      "dest": "/public/$1"
    }
  ]
}
